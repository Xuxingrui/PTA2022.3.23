#include<iostream>
using namespace std;
class  Time
{
public:
	void set_time(void);
	void show_time(void);
	int hour;
	int minute;
	int sec;
};
Time t;
int main()
{
	set_time();
	show_time();
	return 0;
}
int set_time(void)
{
	cin >> t.hour;
	cin >> t.minute;
	cin >> t.sec;
}
int show_time()
{
	cout << t.hour << ":" << t.minute << ":" << t.sec << endl;
}
